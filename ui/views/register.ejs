<%- include ("partials/header") -%>

<div container class="register-cont">
    <div class="reg-content">
        <table class="reg-table">
            <tr>
                <td>
                    <h1 class="reg-heading">Register</h1>
                </td>
                <td>
                    <div class="reg-data">
                        <div class="row g-3 ">
                            <div class="col-auto>
                              <label for="inputUsername" class="col-form-label ">First Name</label>
                            </div>
                            <div class="input-group">
                                <input type="text" class="form-control" id="validationDefaultUsername" style="background-color:transparent;" aria-describedby="passwordHelpInline" required>
                            </div>
                        </div>
                        <label for="inputUsername" class="form-label">Last Name</label>
                        <input type="text" id="inputPassword5" class="form-control reg-col" aria-describedby="passwordHelpBlock">
                        <label for="inputUsername" class="form-label">Password</label>
                        <input type="password" id="inputPassword5" class="form-control reg-col" aria-describedby="passwordHelpBlock">
                        <label for="inputUsername" class="form-label">Confirm Password</label>
                        <input type="password" id="inputPassword5" class="form-control reg-col" aria-describedby="passwordHelpBlock">
                        <button type="button" class="btn btn-outline-light reg-btn">Register</button>
                    </div>
                </td>
            </tr>
        </table>
    </div>

</div> 